<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="purchase_order_form_view_inherit" model="ir.ui.view">
            <field name="name">purchase.order.form.view.inherit</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="x_contract_related"/>
                    <field name="x_annex_related"/>
                    <field name="x_purpose_using"/>
                    <field name="x_using_time"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>