        <record id="tasys_stock_picking_type_cs_executive_rule" model="ir.rule">
            <field name="name">Operation Type: Cs Executive is able to access SO Hoan Tra</field>
            <field name="model_id" ref="stock.model_stock_picking_type"/>
            <field name="domain_force">[('id', 'in', [20,18]) ]</field>
            <field name="groups" eval="[(4, ref('tasys_permission.cs_executive'))]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_unlink" eval="False"/>
        </record>

