            <field name="is_partner_permission" eval="True"/>
            <field name="category_id" ref="tasys_permission.module_tasys_permission"/>
            <field name="implied_ids" eval="[(4,ref('purchase.group_purchase_manager')), (4, ref('account.group_account_manager')), (4, ref('stock.group_stock_manager'))]"/>
        </record>

