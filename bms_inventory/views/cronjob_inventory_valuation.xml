<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_inventory_valuation" model="ir.cron">
        <field name="name">Inventory Valuation</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall">1</field>
        <field name="nextcall" eval="(datetime.utcnow() + timedelta(days=0)).strftime('%Y-%m-%d 00:05:00')"/>
        <field name="model_id" ref="model_bms_inventory_valuation"/>
        <field name="code">model.cron_inventory_valuation()</field>
        <field name="state">code</field>
    </record>
</odoo>
